{
  "name": "opencut editor",
  "version": "2015.10.17.1",
  "manifest_version": 2,
  "description": "A declarative CAM program for generating gcode.",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "app-background.js"
      ]
    }
  },
  "minimum_chrome_version": "31",
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ]
}
